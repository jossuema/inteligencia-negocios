{
	"name": "detencionesFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "detenciones_2019_2024",
						"type": "DatasetReference"
					},
					"name": "datos"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "hechos_detenciones",
